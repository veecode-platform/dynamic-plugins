{
    "name": "@veecode-platform/plugin-veecode-platform-module",
    "version": "0.1.0",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "backstage": {
        "role": "backend-plugin-module",
        "supported-versions": "0.1.3",
        "pluginId": "catalog",
        "pluginPackage": "@veecode-platform/plugin-veecode-platform-module/dynamic"
    },
    "exports": {
        ".": "./src/index.ts",
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "*": {
            "package.json": [
                "package.json"
            ]
        }
    },
    "scripts": {
        "tsc": "tsc",
        "build": "backstage-cli package build",
        "lint:check": "backstage-cli package lint",
        "test": "backstage-cli package test --coverage",
        "clean": "backstage-cli package clean",
        "clean-dynamic-sources": "yarn clean && rm -Rf node_modules",
        "export-dynamic": "janus-cli package export-dynamic-plugin --in-place",
        "build-dynamic-plugin": "npx @janus-idp/cli@latest package export-dynamic-plugin",
        "export-dynamic:clean": "run export-dynamic --clean"
    },
    "dependencies": {
        "@veecode-platform/plugin-veecode-platform-module": "^0.1.3"
    },
    "devDependencies": {
        "@backstage/cli": "0.30.0",
        "@janus-idp/cli": "3.6.1",
        "typescript": "5.8.3"
    },
    "files": [
        "dist",
        "dist-dynamic/*.*",
        "dist-dynamic/dist/**"
    ],
    "keywords": [
        "support:tech-preview",
        "lifecycle:active"
    ]
}
