{
  "name": "@veecode-platform/plugin-database-explorer",
  "version": "0.1.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "frontend-plugin",
    "supported-versions": "0.4.0",
    "pluginId": "database-explorer",
    "pluginPackages": [
      "@veecode-platform/plugin-database-explorer"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "tsc": "tsc",
    "build": "backstage-cli package build",
    "lint:check": "backstage-cli package lint",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "clean": "backstage-cli package clean",
    "export-dynamic": "janus-cli package export-dynamic-plugin --in-place",
    "build-dynamic-plugin": "npx @janus-idp/cli@latest package export-dynamic-plugin",
    "export-dynamic:clean": "run export-dynamic --clean"
  },
  "devDependencies": {
    "@backstage/cli": "0.30.0",
    "@janus-idp/cli": "3.6.1",
    "typescript": "5.8.3"
  },
  "files": [
    "dist",
    "dist-scalprum",
    "app-config.dynamic.yaml"
  ],
  "scalprum": {
    "name": "veecode-platform.plugin-database-explorer-dynamic",
    "exposedModules": {
      "PluginRoot": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veecode-platform",
    "directory": "platform-backstage-plugins"
  },
  "dependencies": {
    "@veecode-platform/plugin-database-explorer": "^0.4.0"
  }
}